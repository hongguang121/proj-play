(define pick
  (λ (a lat)
    (cond ((= a (car lat)) (cdr lat))
          (else (pick a (cdr lat))))))

(define keep-looking
  (λ (a f lat)
    (pick a f)))

(define looking
  (λ (a lat)
    (keep-looking a (pick a lat) lat)))

(define length*
  (λ (lat)
    (cond ((null? lat) 0)
          ((not (pair? lat)) 1)
          (else (+ (length* (car lat))
                   (length* (cdr lat)))))))

(define eternity
  (λ (x)
    (eternity x)))

(define l '(1 2 3 4 5 6 7))

(λ (l)
  (cond ((null? l) 0)
        (else (+ 1 (length* (cdr l))))))

(define god-algo
  (λ (program input)
    (cond
      ((will-stop? (program input)) #t)
      (else #f))))

(define satan-algo
  (λ (program)
    (cond
      ((god-algo program program) (eternity 1))
      (else #t))))

;(satan-algo satan-algo)

