abstract class Inventory
{
    abstract int Value();
}

class Empty extends Inventory 
{ 
    Empty() {}
    int Value() 
    {
      return 0; 
    }
}

class Addition extends Inventory 
{
    int val;
    Inventory rest;
    Addition(int v, Inventory r) 
    { 
      val = v; 
      rest = r; 
    }

    int Value() 
    { 
      return val + rest.Value(); 
    }
}

